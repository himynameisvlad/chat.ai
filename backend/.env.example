# Server Configuration
PORT=3001

# AI Provider Selection (deepseek, claude, or huggingface)
AI_PROVIDER=deepseek

# DeepSeek Configuration (required if AI_PROVIDER=deepseek)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# Claude Configuration (required if AI_PROVIDER=claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_MODEL=claude-3-5-sonnet-20241022
CLAUDE_MAX_TOKENS=4096

# HuggingFace Configuration (required if AI_PROVIDER=huggingface)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
HUGGINGFACE_MODEL=meta-llama/Llama-3.2-3B-Instruct
